@model Microsoft.AspNetCore.Identity.IdentityUser

<h1>Chi tiết</h1>
<table class="table table-striped table-hover ">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Phone</th>
        </tr>
    </thead>

    <tbody>
        <tr>
            <td>@Model.UserName</td>
            <td>@Model.Email</td>
            <td>@Model.PhoneNumber</td>
        </tr>
    </tbody>
</table>